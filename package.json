{
  "name": "cof-database",
  "version": "0.1.0",
  "description": "SQL Server for COF Web Application ",
  "private": true,
  "type": "module",
  "bin": {
    "cof-database": "./build/index.js"
  },
  "files": [
    "build"
  ],
