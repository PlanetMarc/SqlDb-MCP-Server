  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "dotenv": "^17.2.1",
    "mssql": "^11.0.1"
  },
  "devDependencies": {
    "@types/mssql": "^9.1.7",
    "@types/node": "^20.19.10",
    "typescript": "^5.8.2"
  }
}
